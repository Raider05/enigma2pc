@HAVE_XMLTO_FALSE@	  echo "Please install xmlto."; \
@HAVE_XMLTO_FALSE@	  exit 1; \
@HAVE_XMLTO_FALSE@	fi; \
@HAVE_XMLTO_FALSE@	if test x"$(fail_if_missing)" != x"yes"; then \
@HAVE_XMLTO_FALSE@	  touch $@; \
@HAVE_XMLTO_FALSE@	  sleep 1; \
@HAVE_XMLTO_FALSE@	  touch $^; \
@HAVE_XMLTO_FALSE@	fi
	$(AM_V_at)$(SED) -i -e '/<img [^>]*alt=/! s/<img /<img alt="" /g' $@ # we want valid XHTML 1.1

